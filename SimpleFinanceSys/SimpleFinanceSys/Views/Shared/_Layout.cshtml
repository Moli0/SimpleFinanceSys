    <script>
        var vueUser = new Vue({
            el: "#headDiv",
            data: {
                totalAmount: 0,
                cash: 0,
                deposit: 0,
                liabilities: 0,
                userHeadUrl: "",
                userEmail: ""
            }
        });
        var vueUsage = new Vue({
            el: "#usage",
            data: {
                totalAmount: 0,
                cash: 0,
                deposit: 0,
                liabilities: 0,
            }
        });
        $(function () {
            $.ajax({
                url: '/Account/GetUserInfo',
                type: 'GET',
                success: function (res) {
                    if (res.model.state > 0) {
                        vueUser.cash = GetToFixed(res.model.nowMoney,2);
                    } else {
                        vueUser.cash = 0;
                        vueUser.deposit = 0;
                        vueUser.liabilities = 0;
                        vueUser.totalAmount = 0;
                    }
                    vueUser.userHeadUrl = res.model.headUrl;
                    vueUser.userEmail = res.model.email;
                    $.ajax({
                        url: '/Index/GetTotalAmount',
                        type: 'GET',
                        success: function (res) {
                            res.model = JSON.parse(res.model)[0];
                            vueUser.deposit = GetToFixed(res.model.DepositTotal,2);
                            vueUser.liabilities = GetToFixed(res.model.loanTotal, 2);
                            vueUser.totalAmount = GetToFixed(parseFloat(GetToFixed(vueUser.cash, 4)) + parseFloat(GetToFixed(vueUser.deposit, 4)) - parseFloat(GetToFixed(vueUser.liabilities, 4)), 2);
                            vueUsage.totalAmount = vueUser.totalAmount;
                            vueUsage.cash = vueUser.cash;
                            vueUsage.deposit = vueUser.deposit;
                            vueUsage.liabilities = vueUser.liabilities;
                        }
                    });
                }
            });
        });
    </script>
</body>
