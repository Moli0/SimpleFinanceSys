    <script>
        function ChnagePwd() {
            _open({
                type: 0,
                id: "pwdWindow",
                title: "修改密码",
                content: $("#changePwd").html(),
                width: 400,
                height: 400,
                btn: ["确认", "取消"],
                yes: function () {
                    $.ajax({
                        url: '/Account/ChangePwd',
                        data: $("#changePwdForm").serializeArray(),
                        type: "POST",
                        success: function (res) {
                            _alert(res.msg, "ok");
                        }
                    });
                }
            });
        }

        function InitRecordSelect() {
            _form("#payType").bindSelect({
                url: "/Setting/GetTypes?t=" + new Date().getTime(),
                dataType: "string",
                type: "GET",
                id: "id",
                text: "name"
            });
            _form("#payObject").bindSelect({
                url: "/Setting/GetObjects?t=" + new Date().getTime(),
                type: "GET",
                dataType: "string",
                id: "id",
                text: "name"
            });
        }

        function Record() {
            _open({
                type: 0,
                id: "RecordWindow",
                title: "记账",
                content: $("#recordHtml").html(),
                width: 800,
                height: 500,
                btn: ["确认", "取消"],
                yes: function () {
                    $.ajax({
                        url: '/Account/Record',
                        data: $("#recordForm").serializeArray(),
                        type: "POST",
                        success: function (res) {
                            _alert(res.msg, "ok");
                        }
                    });
                }
            });
            InitRecordSelect();
        }

        function LoanRecord(type) {
            var title = "";
            switch (type) {
                case 0: title = "存款"; break;
                case 1: title = "借款"; break;
                case 2: title = "取款"; break;
                default: title = "还款"; break;
            }
            _open({
                type: 0,
                id: "LoanRecordWindow",
                title: title,
                content: $("#LoanHtml").html(),
                width: 800,
                height: 600,
                btn: ["确认", "取消"],
                yes: function () {
                    $.ajax({
                        url: '/Loan/AddLoan',
                        data: $("#loanForm").serializeArray(),
                        type: "POST",
                        success: function (res) {
                            _alert(res.msg, "ok");
                        }
                    });
                }
            });

            $("#recordType").val(type);
            $("#recordTypeSelect").val(type);
            _form("#orderObject").bindSelect({
                url: "/Setting/GetObjects",
                type: "GET",
                dataType: "string",
                id: "id",
                text: "name"
            });
            if (type == 3) {
                $("#idTR").show();
                $("#amountTips").text("*选择关联记录后显示待还金额");
                _form("#oldId").bindSelect({
                    url: '/Loan/GetLoanIdList',
                    type: 'GET',
                    id: "orderId",
                    text: "title",
                });
            }
        }

        function oldIdChange() {
            var orderid = $("#oldId").val();
            $.ajax({
                url: '/Loan/GetModel',
                data: { orderId: orderid },
                type: "Get",
                success: function (res) {
                    $("#amountTips").text("*待还金额为:" + res.model.nowAmount + "元");
                }
            });
        }

        function ChangeSelfMoney() {
            _open({
                type: 0,
                id: "changeMoneyWindow",
                title: "修正资产",
                content: $("#changeMoney").html(),
                width: 400,
                height: 400,
                btn: ["确认", "取消"],
                yes: function () {
                    $.ajax({
                        url: '/Account/ChangeMoney',
                        data: $("#changeMoneyForm").serializeArray(),
                        type: "POST",
                        success: function (res) {
                            _alert(res.msg, "ok");
                        }
                    });
                }
            });
            GetMoney();
        }

        function GetMoney() {
            $.ajax({
                url: '/Account/GetUserInfo',
                type: 'GET',
                success: function (res) {
                    if (res.model.state > 0) {
                        $("#baseAmount").attr("readonly", "readonly");
                        $("#baseAmount").val(res.model.baseAmount);
                        $("#nowMoney").val(res.model.nowMoney);
                    } else {
                        $("#baseAmount").val("");
                        $("#nowMoney").val("0.00");
                    }
                }
            });
        }
    </script>
    <script id="changePwd" type="text/html">
        <div class="row">
            <form id="changePwdForm">
                <table class="table">
                    <tbody>
                        <tr>
                            <th>原密码：</th>
                            <td>
                                <input class="form-control" type="password" name="oldPwd" id="oldPwd" maxlength="28" />
                            </td>
                        </tr>
                        <tr>
                            <th>新密码：</th>
                            <td>
                                <input class="form-control" type="password" name="newPwd" id="newPwd" maxlength="28" />
                            </td>
                        </tr>
                        <tr>
                            <th>确认新密码：</th>
                            <td>
                                <input class="form-control" type="password" name="newPwds" id="newPwds" maxlength="28" />
                            </td>
                        </tr>
                    </tbody>
                </table>
            </form>
        </div>
    </script>
    <script id="changeMoney" type="text/html">
        <div class="row">
            <form id="changeMoneyForm">
                <table class="table">
                    <tbody>
                        <tr>
                            <th>原始金额：</th>
                            <td>
                                <input class="form-control" type="number" name="baseAmount" id="baseAmount" />
                                <span class="font-size-12" style="color: red;">*请注意:此金额一经设定无法再次更改</span>
                            </td>
                        </tr>
                        <tr>
                            <th>当前资产：</th>
                            <td>
                                <input class="form-control" type="number" name="nowMoney" id="nowMoney" />
                            </td>
                        </tr>
                    </tbody>
                </table>
            </form>
        </div>
    </script>
    <script id="recordHtml" type="text/html">
        <div class="row">
            <form id="recordForm">
                <table class="table">
                    <tbody>
                        <tr>
                            <th>记账类型<span class="font-color-red">*</span></th>
                            <td>
                                <select class="form-control" id="orderType" name="orderType">
                                    <option value="1">支出</option>
                                    <option value="0">收入</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <th>记账金额<span class="font-color-red">*</span></th>
                            <td>
                                <input class="form-control" name="amount" id="amount" type="number" />
                            </td>
                        </tr>
                        <tr>
                            <th>消费(收入)类型<span class="font-color-red">*</span></th>
                            <td>
                                <select class="form-control" name="payType" id="payType">
                                    <option value="">----请选择----</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <th>消费(收入)对象<span class="font-color-red">*</span></th>
                            <td>
                                <select class="form-control" name="payObject" id="payObject">
                                    <option value="">----请选择----</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <th>备注</th>
                            <td>
                                <textarea class="form-control" name="remark" id="remark"></textarea>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2"><p class="font-color-red text-indent-2">*账单数据一经记录，不可修改，不可删除，如需修改或删除，请再次反向记账进行金额抹平</p></td>
                        </tr>
                    </tbody>
                </table>
            </form>
        </div>
    </script>
    <script id="LoanHtml" type="text/html">
        <div class="row">
            <form id="loanForm">
                <table class="table">
                    <tbody>
                        <tr>
                            <th>记录类型</th>
                            <td>
                                <input type="hidden" id="recordType" name="recordType" />
                                <select id="recordTypeSelect" name="recordTypeSelect" class="form-control">
                                    <option value="0">存款</option>
                                    <option value="1">借款</option>
                                    <option value="2">取款</option>
                                    <option value="3">还款</option>
                                </select>
                            </td>
                        </tr>
                        <tr id="idTR" class="display-none">
                            <th id="idTH">关联记录</th>
                            <td>
                                <select class="form-control" type="text" name="oldId" id="oldId" readonly="readonly" onchange="oldIdChange()">
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <th>存(借)款对象</th>
                            <td>
                                <select id="orderObject" name="orderObject" class="form-control">
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <th>摘要</th>
                            <td>
                                <input type="text" class="form-control" name="title" id="title" />
                            </td>
                        </tr>
                        <tr>
                            <th>金额</th>
                            <td>
                                <input type="number" class="form-control" name="amount" id="amount" />
                                <span id="amountTips" class="font-color-red">*如有利息产生，建议将其包含在内</span>
                            </td>
                        </tr>
                        <tr class="display-none">
                            <th>利率结算方式</th>
                            <td>
                                <select id="interestRateType" name="interestRateType" class="form-control">
                                    <option value="0">月分期</option>
                                    <option value="1">定期</option>
                                    <option value="2">年结</option>
                                    <option value="3">季度结</option>
                                    <option value="4">月结</option>
                                    <option value="5">日结</option>
                                </select>
                            </td>
                        </tr>
                        <tr class="display-none">
                            <th>利率(%)</th>
                            <td>
                                <input type="number" class="form-control" name="interestRate" id="interestRate" max="100" />
                                <span class="font-color-red">*请以百分比形式填写，如千分之五则填0.5最大值为100</span>
                            </td>
                        </tr>
                        <tr class="display-none">
                            <th>到期日</th>
                            <td>
                                <input type="number" class="form-control" name="endTime" id="endTime" />
                            </td>
                        </tr>
                        <tr>
                            <th>钱款走向</th>
                            <td>
                                <select id="isCash" name="isCash" class="form-control">
                                    <option value="0">非现金余额</option>
                                    <option value="1">现金余额</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <th>备注</th>
                            <td>
                                <textarea type="number" class="form-control" name="remark" id="remark" style="resize:none;" maxlength="1024"></textarea>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </form>
        </div>
    </script>
</head>

